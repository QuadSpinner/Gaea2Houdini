{
version 0.8
python	[ 0	locks=0 ]	(	"import os
from importlib import reload

from gaea import processing_utils

reload(processing_utils)

node = hou.pwd()
geo = node.geometry()

cache_dir = node.parent().parm(\"cachedir\").evalAsString()
extra_data = node.parent().parm(\"extra_data\").evalAsString()
os.makedirs(cache_dir, exist_ok=True)

heightfield_resolution = geo.attribValue(\"_gaea_resolution\")
heightfield_range = geo.attribValue(\"_gaea_range\")
parms_present = False
try:
    export_parms = node.parent().parmsInFolder(
        (
            processing_utils.GENERATED_FOLDERS_LABEL[1],
            processing_utils.IMPORT_BINDING_LABEL,
        )
    )
    export_layers = {
        parm.name(): {\"layer_name\": parm.evalAsString()} for parm in export_parms
    }
    import_parms = node.parent().parmsInFolder(
        (
            processing_utils.GENERATED_FOLDERS_LABEL[1],
            processing_utils.EXPORT_BINDING_LABEL,
        )
    )
    import_layers = {
        parm.name(): {\"layer_name\": parm.evalAsString()} for parm in import_parms
    }
    parms_present = True
except hou.OperationFailed:
    pass

if not parms_present:
    raise hou.NodeError(processing_utils.NodeMessages.NO_PARAMETERS)

# Export Layers
processing_utils.export_houdini_layers(
    geometry=geo,
    cache_dir=cache_dir,
    layer_data=export_layers,
    extra_data=extra_data,
    value_range=heightfield_range,
)

# Call Gaea Cook
success = processing_utils.run_gaea_swarm(
    node=node.parent(),
    cache_dir=cache_dir,
    extra_data=extra_data,
    in_layer_mappings=import_layers,
    out_layer_mappings=export_layers,
    resolution=heightfield_resolution,
)
"	)
maintainstate	[ 0	locks=0 ]	(	"off"	)
}

