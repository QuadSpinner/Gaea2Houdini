Content-Type: text/plain

{
version 0.8
python	[ 0	locks=0 ]	(	"from gaea import processing_utils

#TODO: Remove
from importlib import reload
reload(processing_utils)


node = hou.pwd()
geo = node.geometry()

allowed_resolutions = processing_utils.SUPPORTED_TERRAIN_RESOLUTIONS
geo.addAttrib(hou.attribType.Global, \"_gaea_resolution\", 0, transform_as_normal=False, create_local_variable=False)
geo.addAttrib(hou.attribType.Global, \"_gaea_range\", (0.0,0.0), transform_as_normal=False, create_local_variable=False)
min = None
max = None
for prim in geo.prims():
    name = prim.attribValue(\"name\")
    if not prim.isHeightField() and name != \"mask\":
        continue
        
    resx, resy, _ = prim.resolution()
    resx = int(resx)
    resy = int(resy)
    
    if resx != resy:
        raise RuntimeError(processing_utils.NodeMessages.LAYER_NOT_SQUARE)
        
    if resx not in allowed_resolutions or resy not in allowed_resolutions:
        raise RuntimeError(processing_utils.NodeMessages.LAYER_NOT_POW2)
    
    geo.setGlobalAttribValue(\"_gaea_resolution\", resx)
    volume_min = prim.volumeMin()
    volume_max = prim.volumeMax()
    if min is None or volume_min < min:
       min = volume_min
    if max is None or volume_max > max:
       max = volume_max

geo.setGlobalAttribValue(\"_gaea_range\", (min, max))"	)
maintainstate	[ 0	locks=0 ]	(	"off"	)
}

